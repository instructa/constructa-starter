{% set derived_keys = [
  'DATABASE_URL',
  'REDIS_URL'
] %}

{% for key, value in env_map.items() %}
{% if key not in derived_keys %}
{{ key }}={{ value }}
{% endif %}
{% endfor %}

# Derived URLs (rendered with URL-encoding)
DATABASE_URL=postgresql://{{ env_map.get('POSTGRES_USER') | urlencode }}:{{ env_map.get('POSTGRES_PASSWORD') | urlencode }}@{{ env_map.get('POSTGRES_HOST', 'db') }}:{{ env_map.get('POSTGRES_PORT', '5432') }}/{{ env_map.get('POSTGRES_DB') | urlencode }}
REDIS_URL=redis://:{{ env_map.get('REDIS_PASSWORD') | urlencode }}@{{ env_map.get('REDIS_HOST', 'redis') }}:{{ env_map.get('REDIS_PORT', '6379') }}
